    "effectful",
    "Eilam",
    "endo",
    "Endo",
    "eslintcache",
    "expando",
    "extendedglob",
    "extglob",
    "fanin",
    "fanout",
    "Fira",
    "fivepoverhang",
    "fontname",
    "fpath",
    "graphviz",
    "grey",
    "haag",
    "Haag",
    "Hanoch",
    "hylo",
    "invhouse",
    "invtrapezium",
    "invtriangle",
    "jiti",
    "jsons",
    "kahn",
    "kinded",
    "larrow",
    "lpromoter",
    "mapprop",
    "mapprops",
    "mathfunc",
    "Mcircle",
    "Mdiamond",
    "modprop",
    "modstyle",
    "monoid",
    "Monoid",
    "msgpackr",
    "Msquare",
    "multibyte",
    "newrank",
    "Nitsan",
    "nodenext",
